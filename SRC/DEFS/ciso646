#ifndef __CISO646
#	define __CISO646

namespace std {


#define and     &&
#define and_eq  &=
#define bitand  &
#define bitor   |
#define compl   ~
#define not     !
#define not_eq  !=
#define or      ||
#define or_eq   |=
#define xor     ^
#define xor_eq  ^=



} /* namespace std */

#endif /* __CISO646 */
