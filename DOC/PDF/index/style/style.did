    constant:	CASEDEX      unsigned-integer "0"
    fixwidth:	STEMDEX    3 unsigned-integer
    fixwidth:	SOUNDEX    3 unsigned-integer
  }
  data-table:	dis
  {
    fixwidth:	NUMWORDS   2 unsigned-integer
  }
}
