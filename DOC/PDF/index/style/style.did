  data-table:	dix
  {
    fixwidth:	FWIDX	   3 text
  }
  data-table:	dif
  {
    constant:	_DBVERSION	text "vdk060"
    varwidth:	FWWORD	 diw
    fixwidth:   FWWORD_OF  3 unsigned-integer
    fixwidth:	FWWORD_SZ  1 unsigned-integer

    varwidth:	FWCOUNT	 div
    fixwidth:   FWCOUNT_OF  4 unsigned-integer
